{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@react-three/drei": "^9.46.5",
    "@react-three/fiber": "^8.9.1",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/serve": "^1.8.2",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.147.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/three": "^0.146.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
